<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - GEMC大气数据分析</title>

    <link rel="stylesheet" type="text/css" href="/static/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/static/css/dashboard.css" />
    <script src="/static/scripts/dashboard.js"></script>
    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @@media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }
    </style>
  </head>
  <body>

<header class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
  <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" href="/">GEMC大气数据分析</a>
  <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <ul class="navbar-nav w-100"></ul>
  <ul class="navbar-nav px-3">
    <li class="nav-item text-nowrap">
      <a class="nav-link" href="#">Sign In</a>
    </li>
  </ul>
  <ul class="navbar-nav px-3">
    <li class="nav-item text-nowrap">
      <a class="nav-link" href="#">Sign out</a>
    </li>
  </ul>
</header>

<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link" aria-current="page" href="/admin">
              <span data-feather="home"></span>
              首页
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">
              <span data-feather="file"></span>
              关于
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">
              <span data-feather="shopping-cart"></span>
              Contact
            </a>
          </li>
        </ul>

        <h6 class="sidebar-heading d-flex justify-content-between align-items-center px-3 mt-4 mb-1 text-muted">
          <span>功能项</span>
          <a class="link-secondary" href="#" aria-label="Add a new report">
            <span data-feather="plus-circle"></span>
          </a>
        </h6>
        <ul class="nav flex-column mb-2">
          <li class="nav-item">
            <a class="nav-link" href="#">
              <span data-feather="file-text"></span>
              今日动态(X)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/citydata">
              <span data-feather="file-text"></span>
              中心城市(X)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/davgdata">
              <span data-feather="file-text"></span>
              国控点日均值(Y)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/trendgraph">
              <span data-feather="file-text"></span>
              趋势图(X)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">
              <span data-feather="file-text"></span>
              排名(X)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/statistics">
              <span data-feather="file-text"></span>
              统计（多点）(Y)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/yearavg">
              <span data-feather="file-text"></span>
              年均值(Y)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/monthavg">
              <span data-feather="file-text"></span>
              月均值(X)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/dayavg">
              <span data-feather="file-text"></span>
              日均值(Y)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/aqiheatmap">
              <span data-feather="file-text"></span>
              AQI热力图(Y)
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/WeatherForecast">
              <span data-feather="file-text"></span>
              预测预报
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/admin/WeatherForecastDB">
              <span data-feather="file-text"></span>
              预测预报DB
            </a>
          </li>
        </ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">@ViewData["Title"].</h1>
      </div>
		@RenderBody()
        <hr />

        <footer>
            <p>&copy; HKL 2021 ~ 2023 - - GEMC大气数据分析</p>
        </footer>
    </main>
  </div>
</div>

    <script src="/static/scripts/bootstrap.bundle.min.js"></script>
    <script src="/static/scripts/jquery-1.10.2.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/feather-icons@4.28.0/dist/feather.min.js" integrity="sha384-uO3SXW5IuS1ZpFPKugNNWqTZRRglnUJK6UAZ/gxOX80nxEkN9NcGZTftn6RzhGWE" crossorigin="anonymous"></script><script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js" integrity="sha384-zNy6FEbO50N+Cg5wap8IKA4M/ZnLJgzc6w2NqACZaK0u0FXfOWRRJOnQtpZun8ha" crossorigin="anonymous"></script>
    <script src="/static/scripts/gemcgas.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
  </body>
</html>



